{
  "manifest_version": 3,
  "name": "Bookmark Sorter",
  "description": "Sort Bookmark",
  "version": "0.1",
  "action": {
    "default_popup": "static/index.html"
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  }
}
